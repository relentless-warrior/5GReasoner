TRANS
((next(_B0_) -> (next(ite) = (nB0 + 1))) & (! (next(_B0_)) -> (next(ite) = nB0))) &
((next(_B1_) -> (next(ite2) = (nB1 + 1))) & (! (next(_B1_)) -> (next(ite2) = nB1))) &
((next(_S_) -> (next(ite3) = (nS + 1))) & (! (next(_S_)) -> (next(ite3) = nS)));




