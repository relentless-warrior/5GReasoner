MODULE controleur (nB,nS)
VAR
diff: integer;
avance: boolean;
retard: boolean;
ite: boolean;
flby: boolean;
ite2: boolean;
flby2: boolean;

ASSIGN
diff := (nB - nS);
avance := flby;
retard := flby2;
init(flby) := FALSE;
next(flby) := next(ite);
init(flby2) := FALSE;
next(flby2) := next(ite2);

TRANS
((! (avance) -> (next(ite) = (next(diff) >= 10))) & (! (! (avance)) -> (next(ite) = (next(diff) > 0)))) &
((! (retard) -> (next(ite2) = (next(diff) <= - (10)))) & (! (! (retard)) -> (next(ite2) = (next(diff) < 0))));




MODULE hypothese (_B_,_S_,avance,retard)
VAR
