MODULE Sofar (_X_)
VAR
Sofar: boolean;
flby: boolean;

ASSIGN
Sofar := flby;
init(flby) := _X_;
next(flby) := (next(_X_) | Sofar);




MODULE Age (p)
VAR
age_of_p: integer;
ite: integer;
flby: integer;

ASSIGN
age_of_p := flby;
init(flby) := 0;
next(flby) := next(ite);

TRANS
((p -> (next(ite) = (age_of_p + 1))) & (! (p) -> (next(ite) = 0)));




MODULE main
VAR
_OK_: boolean;
k: integer;
m: integer;
env: boolean;
flby: integer;
flby2: integer;
flby3: boolean;
param_ik: integer;
param_im: integer;
param_p: boolean;
param_q: boolean;
param_r: boolean;
call_Age: Age (param_p);
call_Age2: Age (param_q);
call_Sofar: Sofar (((((k >= 1) & (m >= 1)) & ((call_Age.age_of_p >= k) -> param_q)) & ((call_Age2.age_of_p >= m) -> param_r)));
call_Age3: Age (param_p);

ASSIGN
_OK_ := (env -> flby3);
k := flby;
m := flby2;
env := call_Sofar.Sofar;
init(flby) := param_ik;
next(flby) := k;
init(flby2) := param_im;
next(flby2) := m;
init(flby3) := TRUE;
next(flby3) := ((next(call_Age3.age_of_p) >= (next(k) + next(m))) -> next(param_r));

INVARSPEC (_OK_ = TRUE)


